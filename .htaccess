RewriteEngine on

RewriteRule ^signup/([a-zA-Z0-9_-]+)$ signup.html?code=$1
RewriteRule ^signup/([a-zA-Z0-9_-]+)/$ signup.html?code=$1


RewriteCond %{REQUEST_FILENAME} !-f
RewriteRule ^([^/]+)/$ $1.html
RewriteRule ^([^/]+)/([^/]+)/$ /$1/$2.html
RewriteCond %{REQUEST_FILENAME} !-f
RewriteCond %{REQUEST_FILENAME} !-d
RewriteCond %{REQUEST_URI} !(\.[a-zA-Z0-9]{1,5}|/)$
RewriteRule (.*)$ /$1/ [R=301,L]